

<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<div class="row">
  <div class="col-md-12">
    <div class="listing">
      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>CI</th>
              <th>Nombres</th>
              <th>Apellidos</th>
              <th>Empresa</th>
              <th>Tipo</th>
              <th>Habilitado</th>
              <th colspan="3"></th>
            </tr>
        </thead>
      
        <tbody>
                <%= render @usuarios %>
        </tbody>
      </table>

      </div>
    </div>
  </div>
  </div>

<br>
<div class="col-md-12">
  <div class="btn-group btn-group-justified">
    <%= link_to 'Nuevo Usuario', new_usuario_path, class: 'btn btn-default'  %>
  
    <%= link_to 'Volver', root_path, class: 'btn btn-default' %>

  </div>
</div>