<tr>
  <td><%= franja.fecha.to_formatted_s(:short)  %></td>
  <td><%= franja.primera_hasta %></td>
  <td><%= franja.primera_precio %></td>
  <td><%= franja.segunda_hasta %></td>
  <td><%= franja.segunda_precio %></td>
  <td><%= franja.segunda_hasta %></td>
  <td><%= franja.tercera_precio %></td>

  <td>
  <div class="btn-group">
    <%= link_to '', franja, class: 'btn btn-success btn-sm glyphicon glyphicon-eye-open', :title => "Ver" %>
    <%= link_to '', edit_franja_path(franja), class: 'btn btn-default btn-sm glyphicon glyphicon-pencil', :title => "Editar" %>
    <%= link_to '',
                franja,
                method: :delete,
                class: 'btn btn-warning btn-sm glyphicon glyphicon-remove',
                data: { confirm: 'Esta seguro de eliminar esta franja?' }, :title => "Eliminar" %>
  </div>
  </td>
</tr>