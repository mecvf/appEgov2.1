<tr>
  <td><%= empresa.rut %></td>
  <td><%= empresa.nombre %></td>
  <td><%= empresa.razon_social %></td>
  <td><%= empresa.telefono %></td>
  <td><%= empresa.direccion %></td>
  <td><%= empresa.email %></td>
  <td>
  <div>
  <div class="btn-group">

    
            <%= link_to '',empresa, class: 'btn btn-success btn-sm glyphicon glyphicon-eye-open', :title => "Ver" %>
            
    
    <%= link_to '', edit_empresa_path(empresa), class: 'btn btn-default btn-sm glyphicon glyphicon-pencil', :title => "Editar" %>
    
    
    <%= link_to '',
                empresa,
                method: :delete,
                class: 'btn btn-warning btn-sm glyphicon glyphicon-remove',
                data: { confirm: 'Esta seguro de eliminar esta empresa?' }, :title => "Eliminar" %>
  
  
  </div>
  </td>
</tr>